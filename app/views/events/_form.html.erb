<%= semantic_form_for @event do |f| %>
  <%= f.inputs do %>
    <%#= f.input :employee %>
    <%= f.input :type %>
    <%= f.input :name %>
    <%= f.input :start_date %>
    <%= f.input :end_date %>
    <%= f.input :away %>
  <% end %>

  <%= f.actions do %>
    <%= f.action :submit, :as => :input %>
  <% end %>
<% end %>
